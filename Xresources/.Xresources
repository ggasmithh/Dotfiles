!Colors
#define basefg #999999
#define basebg #000000

#define base0 #303030
#define base1 #999999
#define base2 #b3b3b3
#define base3 #e6e6e6
#define base4 #8c0000
#define base5 #990000
#define base6 #cc0000
#define base7 #ff0000
#define base8 base0
#define base9 base1
#define base10 base2
#define base11 base3
#define base12 base4
#define base13 base5
#define base14 base6
#define base15 base7

!URxvt
!------------------------------------
*foreground:		basefg
*background:		basebg
*cursor:    		basefg

*color0:   		base0 
*color1:   		base1
*color2:   		base2
*color3:   		base3 
*color4:   		base4 
*color5:   		base5 
*color6:   		base6 
*color7:   		base7 
*color8:   		base8 
*color9:   		base9 
*color10:  		base10 
*color11:  		base11 
*color12:  		base12 
*color13:  		base13 
*color14:  		base14 
*color15:  		base15 

URxvt.font:		xft:FantasqueSansMono Nerd Font Mono:size=10
URxvt.scrollBar:	false
URxvt.internalBorder:	25
URxvt.cursorUnderline:	false

!URxvt.perl-ext-common:	default

!Rofi
!------------------------------------
rofi.color-window:	basebg, basefg

rofi.color-normal:	basebg, basefg, base0, base1, basefg
!rofi.color-active:	base3, base15, base0, base0, base3
!rofi.color-urgent:	base0, base3, base0, base0, base3

rofi.bw:		5
rofi.font:		FantasqueSansMono Nerd Font 10
rofi.columns:		1
rofi.separator-style:	none
rofi.hide-scrollbar:	true
rofi.location:		1
rofi.yoffset:		25
rofi.xoffset:		25
rofi.padding:		25
rofi.fullscreen:	false
rofi.width:		150
rofi.lines:		0
rofi.case-sensitive:	false

Xft.autohint:		0
Xft.lcdfilter:		lcddefault
Xft.hintstyle:		hintslight
Xft.hinting:		1
Xft.rgba:		rgb
Xft.dpi:		110		

!2bwm
!-------------------------------
twobwm.border_width:		5
twobwm.outer_border:		5
twobwm.focus_color:		basefg
twobwm.unfocus_color:		basebg
twobwm.fixed_color:		base4
twobwm.unkill_color:		base5
twobwm.fixed_unkill_color:	base6

